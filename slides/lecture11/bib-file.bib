@article{tversky83,
  title =        {{Categories of Environmental Scenes}},
  author =       {B. Tversky and K. Hemenway},
  year =         1983,
  journal =      {Cognitive Psychology},
  volume =       15,
  pages =        {121--149},
  annote =       {This paper summarizes the findings of two cognitive
                  psychologists about which scene categories humans use. At
                  first, the authors introduce the notion of the superordinate,
                  basic, and subordinate level in the categorization of scenes.
                  Their goal is to build a taxonomy for scene categories and to
                  identify the basic or preferred level of categorization for
                  scenes. They carry out a set of psychophysical experiments
                  which result in a taxonomy of environmental scenes. Their
                  research indicates that knowledge about scenes has a
                  hierarchical organization with an intermediate or \emph{basic}
                  level. In addition, the experiments reveal attributes,
                  activities, and parts for each category, especially the basic
                  level categories.}
}

