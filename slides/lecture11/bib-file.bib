@inproceedings{sherrah:00:a,
  title =        {{Tracking Discontinuous Motion using Bayesian Inference}},
  author =       {Jamie Sherrah and Shaogang Gong},
  year =         2000,
  booktitle =    {ECCV},
  address =      {Dublin, Ireland},
  volume =       2,
  pages =        {150--166},
  url =
                  {http://www.dcs.qmul.ac.uk/~sgg/papers/Sherrah-Gong-eccv00.pdf.gz},
  annote =       {This paper presents a head/hand tracker for human computer
                  interfaces (HCI). In contrast to similar systems, this
                  proposal replaces the spatio-temporal prediction for upholding
                  the continuity assumption with Bayesian reasoning about
                  body-part associations based on contextual knowledge. After
                  localizing the subject's head with a mean shift tracker, a
                  Bayesian network s used for reasoning about the most probable
                  association of skin color blobs and hands. Due to this
                  reasoning, the system is able to deal reliably with fast,
                  discontinuous movements of the subject's hands.},
  keywords =     {human computer interface, tracking, Bayesian network, mean
                  shift},
  bdsk-url-1 =
                  {http://www.dcs.qmul.ac.uk/~sgg/papers/Sherrah-Gong-eccv00.pdf.gz}
}

@inproceedings{sherrah:00:b,
  title =        {{Resolving Visual Uncertainty and occlusion through
                  Probabilistic Reasoning}},
  author =       {Jamie Sherrah and Shaogang Gong},
  year =         2000,
  booktitle =    {BMVC},
  url =          {http://www.bmva.ac.uk/bmvc/2000/papers/p26.pdf},
  annote =       {Brief summary paper comparing two approaches to body part
                  tracking: The first approach models the scene-specific a
                  priori knowledge bt means of a Bayesian network, the second
                  approach uses particle filtering with an exclusion principle
                  for tracking the body parts. Both methods are able to resolve
                  uncertainty stemming from ambiguities in visual data but the
                  Bayesian approach usually outperforms the particle
                  filter-based method. This is mainly due to the more
                  sophisticated knowledge of the scenario represented by the
                  Bayes network. Besides this comparison of two methods, this
                  paper provides also a very short introduction/overview of
                  Bayesian networks.},
  keywords =     {Bayesian network, Condensation, particle filter, tracking},
  bdsk-url-1 =   {http://www.bmva.ac.uk/bmvc/2000/papers/p26.pdf}
}

@inproceedings{sherrah:01:a,
  title =        {{Continuous Global Evidence-Based Bayesian Modality Fusion for
                  Simultaneous Tracking of Multiple Objects}},
  author =       {Jamie Sherrah and Shaogang Gong},
  year =         2001,
  booktitle =    {ICCV},
  publisher =    {IEEE Computer Society, Los Alamos CA},
  address =      {Vancouver, British Columbia, Canada},
  volume =       2,
  pages =        {42--49},
  url =
                  {http://www.dcs.qmul.ac.uk/~sgg/papers/iccv01-sherrah-gong.pdf.gz},
  annote =       {Detailed description how one can fuse multiple modalities
                  using Bayesian networks. In contrast to classical cue fusion
                  approaches using a voting approach, continuous Bayesian
                  modality fusion (CBMF) combines multiple cues by reasoning
                  about the individual cue's reliability. Continuous global
                  evidence-based Bayesian modality fusion has four main
                  advantages: 1) Continuous sampling reflects the complexity of
                  Bayesian inference most appropriately. 2) Using global
                  evidence allows to consider all observations from a single
                  modality for inference. Most voting approaches take only a
                  single observation per modality into consideration. 3)
                  Bayesian inference allows to introduce new evidence one by
                  one. This provides the ability to distinguish between
                  mandatory modalities required for object tracking and optional
                  modalities which give only further hints about the presence of
                  an object. Furthermore, the nature of Bayesian modality fusion
                  allows to implement on-demand computation for computationally
                  expensive modalities that are only computet selectively for
                  disambiguating ambigous object localization.},
  keywords =     {modality fusion, tracking, Bayesian network},
  bdsk-url-1 =
                  {http://www.dcs.qmul.ac.uk/~sgg/papers/iccv01-sherrah-gong.pdf.gz}
}

