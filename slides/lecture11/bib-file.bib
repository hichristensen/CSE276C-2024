@article{ridderinkhof04:_role_medial_front_cortex_cognit_contr,
  title =        {{The Role of Medial Frontal Cortex in Cognitive Control}},
  author =       {K.R. Ridderinkhof and M. Ullsperger and E.A. Crone and S.
                  Nieuwenhuis},
  year =         2004,
  month =        oct,
  journal =      {Science},
  volume =       306,
  number =       5695,
  pages =        {443--447},
  annote =       {Adaptive goal-directed behaviour involves monitoring of
                  on-going actions and performance outcomes, and subsequent
                  adjustment of behaviour and learning. We evaluate new findings
                  in cognitive neuroscience concerning cortical interaction that
                  subserve the recruitment and implementations of such cognitive
                  control. A review of rpimate and human studies, along with
                  meta-analysis of the human functional neuro-imaging literature
                  suggest that the detection of unfavourable outcomes, response
                  errors, response conflict, and decision uncertainty elicits
                  largely overlapping clusters of activiation loci in an
                  extensive part of the poisterior madial frontal cortex (pMFC).
                  So pMFC might be the loci of the regolatory process that
                  implements preformance adjustments.}
}

