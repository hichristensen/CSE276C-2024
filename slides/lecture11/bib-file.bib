@inproceedings{maccormick:00:b,
  title =        {{Partitioned sampling, articulated objects, and
                  interface-quality hand tracking}},
  author =       {John MacCormick and Michael Isard},
  year =         2000,
  booktitle =    {ECCV},
  series =       {LNCS},
  volume =       1843,
  pages =        {3--19},
  url =
                  {http://link.springer.de/link/service/series/0558/bibs/1843/18430003.htm},
  annote =       {The authors present another extension to the original
                  Condensation algorithm \cite{isard:98:a}. Using partitioned
                  sampling can effectively reduce the computational demands for
                  a given problem. In principle, partitioned sampling is a
                  statistical analogy \emph{hierarchical search}. Articulated
                  objects are therefore suitable in particular for this kind of
                  (multi-)object tracker. Besides partitioned sampling, the
                  paper introduces the notion of \emph{survival diagnostics} and
                  \emph{survival rate}. Both quantities can be used for
                  assessing the efficacy of a particle filter. Survival
                  diagnostic -- a.k.a. \emph{effective sample set size}
                  \cite{doucet:02:a} -- measures the effective sample set size
                  which has to be within the boundaries of 1 (one sample has
                  weight 1 all others have weight 0) and $n$ (all samples have
                  equal weight $1/n$). The survival rate is a property of a
                  given sample set meassuring the over-all tracking performance.
                  More technical details about partitioned sampling for hand
                  tracking can be found in \cite{isard:00:a}.},
  keywords =     {Particle Filter, Partitioned Sampling, Tracking, Articulated
                  Objects, Survival Diagnostics, Effective Sample Set Size},
  bdsk-url-1 =
                  {http://link.springer.de/link/service/series/0558/bibs/1843/18430003.htm}
}

