@inproceedings{Han04accv,
  title =        {{Sequential Kernel Density Approximation Through Mode
                  Propagation: Application to Background Modeling}},
  author =       {B. Han and D. Comaniciu and L. Davis},
  year =         2004,
  month =        feb,
  booktitle =    {ACCV},
  abstract =     {Density-based modeling of visual features is very common in
                  computer vision, either by using non-parametric techniques or
                  through representing the underlying density function as a
                  weighted sum of Gaussians. A number of real-time tasks such as
                  background modeling or modeling the appearance of a moving
                  target require sequential density estimation, where new data
                  is incorporated in the model as it becomes available.
                  Nevertheless, current methods for updating the density
                  function either lack flexibility, by fixing the number of
                  Gaussians in the mixture, or require large memory amounts, by
                  maintaining a non-parametric representation of the den- sity.
                  This paper presents an efficient method for recursive density
                  approximation that relies on the propagation of the density
                  modes. At each time step, the modes of the density are
                  re-estimated and a Gaussian component is assigned to each
                  mode. The covariance of each component is derived from the
                  Hessian matrix estimated at the mode location. To detect the
                  modes we employ the variable-bandwidth mean shift. While the
                  proposed density representation is memory efficient (which is
                  typical for mixture densities), it inherits the flexibility of
                  non-parametric methods, by allowing the number of modes to
                  adapt in time. We show that the same mode propagation
                  principle applies for subspaces derived from eigen analysis.
                  Extensive experimental background modeling results demonstrate
                  the performance of the method.},
  c-houdan =     {mean-shift based KDE for initialization + mode propagation for
                  updating the background model, good related work describing},
  date-added =   {2014-12-31 17:39:21 +0000},
  date-modified ={2014-12-31 17:39:23 +0000}
}

