@article{poggio04:_gener,
  title =        {{Generalisation in vision and motor control}},
  author =       {T. Poggio and E. Bizzi},
  year =         2004,
  month =        oct,
  journal =      {Nature},
  volume =       431,
  number =       7010,
  pages =        {768--773},
  annote =       {Learning is more than memory. it is not simply the building of
                  look-up tables of labelled images or a phone like dictionary
                  list of motor acts and the corresponding sequence of muscle
                  activities. central to intelligence is the ability to predict,
                  that it, to generalize to new situations, beyond the memory of
                  specific situations. The key to generalisation, in turn, isz
                  the architecture of the system, more than the rules of the
                  synaptic plasticity. We propose a specific architecture for
                  generalisation for both motor and visual imagery, and argue
                  for a canonical microcircuit underlying visual and motor
                  learning. The learning is based on linear
                  classifiers/estimator, with importance/uncertainty based
                  weighting!}
}

