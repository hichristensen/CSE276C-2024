@article{Kelly07jep,
  title =        {{Sensorimotor alignment effects in the learning environment
                  and in novel environments}},
  author =       {J. Kelly and M. Avraamides and J. Loomis},
  year =         2007,
  journal =      {Journal of Experimental Psychology: Learning, Memory, and
