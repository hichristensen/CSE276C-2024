@article{oliva01,
  title =        {{Modeling the Shape of the Scene: A Holistic Representation of
                  the Spatial Envelope}},
  author =       {A. Oliva and A. Torralba},
  year =         2001,
  month =        mar,
  journal =      {International Journal of Computer Vision},
  volume =       42,
  number =       3,
  pages =        {145--175},
  annote =       {Summarizing
                  \cite{oliva01}\cite{oliva02}\cite{torralba01a}\cite{torralba03a}:
                  The work by Oliva and Torralba is based psychophysically on
                  the PhD work of Aude Oliva \cite{oliva00}\cite{schyns94}. Even
                  when viewing scenes only for a very short time period, humans
                  are able to extract enough visual information to accurately
                  recognize its categorical properties. The authors thus use the
                  discrete Fourier transform (DFT) and the windowed DFT in order
                  to describe and to learn the spatial properties of certain
                  scene categories. In \cite{oliva01}, they learn the degrees of
                  naturalness, of openness, of roughness, of expansion and of
                  ruggedness (spatial envelope properties) in order to sort
                  unseen images along these axes. In \cite{torralba01a}, the DFT
                  and the windowed DFT are used to determine the global depth of
                  a scene without absolute depth measurements. In general, the
                  goal of the authors' work is to generate scene-centered
                  descriptions from the spatial envelope properties
                  \cite{oliva02}\cite{torralba03a}.}
}

