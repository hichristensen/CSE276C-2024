@article{vailaya98,
  title =        {{On Image Classification: City vs. Landscape}},
  author =       {A. Vailaya and A. Jain and H.J. Zhang},
  year =         1998,
  month =        dec,
  journal =      {Pattern Recognition},
  volume =       31,
  number =       12,
  pages =        {1921--1935},
  annote =       {In this paper, the authors also carry out experiments with
                  human subjects. The subjects are asked to sort a set of images
                  into meaningful categories. The evaluation of these categories
                  led to a hierarchical organization of 11 categories. The
                  authors simplify this hierarchy into multiple binary
                  classifications: indoor vs. outdoor, outdoor into city vs.
                  landscape, landscape into sunset vs. mountain/forest,
                  mountain/forest into mountain vs. forest. For the
                  classification they employ a Bayesian framework and vector
                  quantization. In this work, the authors implement only the
                  city vs. landscape classification and landscape into
                  sunset/forest/mountain classes. Using features such as edge
                  direction histograms, color histograms, edge coherence, and
                  color coherence, they reach classification rates from 75\% to
                  93\% depending on the features and the task.}
}

