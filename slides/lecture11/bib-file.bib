@article{Nadel04neuropsych,
  title =        {{The spatial brain}},
  author =       {Nadel, Lynn and Hardt, Oliver},
  year =         2004,
  month =        jul,
  journal =      {Neuropsychology},
  volume =       18,
  number =       3,
  pages =        {473--476},
  abstract =     {Themes emerging from the collection of articles in the Special
                  Section on Long-Term Spatial Memory include the notion of
                  multiple spatial systems, the relation between spatial
                  representations and episodic memory, the role of context, and
                  the neural systems involved in space. The authors conclude
                  that distinguishing between egocentric and allocentric spatial
                  systems makes sense of both behavioral and neurobiological
                  data. The special role of the hippocampal system in
                  allocentric space, and as a consequence, in context, suggests
                  how a spatial system might end up central to the ability to
                  remember episodes.},
  c-richard =    {A review of literature on spatial representation. Says that
                  egocentric and allocentric representations are separate and
                  distinct systems in the brain. Also that the hippocampus may
                  be involved in linking episodic memory with spatial memory.},
  date-added =   {2014-12-31 17:39:21 +0000},
  date-modified ={2014-12-31 17:39:23 +0000}
}

