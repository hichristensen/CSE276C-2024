@inproceedings{pacchierotti06:_iros,
  title =        {{Design of an Office-Guide Robot for Social Interaction
                  Studies}},
  author =       {E. Pacchierotti and H. I. Christensen and P. Jensfelt},
  year =         2006,
  month =        oct,
  booktitle =    {Intl Conf on Intelligent Robots and Systems (IROS)},
  address =      {Beijing, China},
  abstract =     {In this paper, the design of an office-guide robot for social
                  interaction studies is presented. We are interested in
                  studying the impact of passage behaviours in casual
                  encounters. While the system offers assistance in locating the
                  appropriate office that a visitor wants to reach, it is
                  expected to engage in a passing behaviour to allow free
                  passage for other persons that it may encounter. Through use
                  of such an approach it is possible to study the effect of
                  social interaction in a situation that is much more natural
                  than out-of-context user studies. The system has been tested
                  in an early evaluation phase when it worked for almost 7
                  hours. A total of 64 interactions with people were registered
                  and 13 passage behaviors were performed to conclude that this
                  framework can be successfully used for the evaluationof
                  passing behaviors in natural contexts of operation.},
  organization = {RSJ/IEEE},
  pdf =
                  {http://www.nada.kth.se/~hic/hic-papers/iros_06_pacchierotti_final.pdf}
}

@inproceedings{pacchierotti06:_roman,
  title =        {{Evaluation of Passing Distance for Social Robots}},
  author =       {E. Pacchierotti and H. I. Christensen and P. Jensfelt},
  year =         2006,
  month =        sep,
  booktitle =    {Workshop on Robot and Human Interactive Communication (ROMAN)},
  address =      {Hertfordshire, UK},
  abstract =     {Casual encounters with mobile robots for non- experts can be a
                  challenge due to lack of an interaction model. The present
                  work is based on the rules from proxemics which are used to
                  design a passing strategy. In narrow corridors the lateral
                  distance of passage is a key parameter to consider. An
                  implemented system has been used in a small study to verify
                  the basic parametric design for such a system. In total 10
                  subjects evaluated variations in proxemics for encounters with
                  a robot in a corridor setting. The user feedback indicates
                  that entering the intimate sphere of people is less
                  comfortable, however a too significant avoidance is also
                  considered unnecessary. Adequate signaling of avoidance is a
                  behaviour that must be carefully tuned.},
  editor =       {K. Dautenhahn},
  organization = {IEEE},
  pdf =
                  {http://www.nada.kth.se/~hic/hic-papers/roman06-pacchierotti-final.pdf}
}

